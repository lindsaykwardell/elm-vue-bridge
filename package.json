{
  "name": "elm-vue-bridge",
  "version": "1.0.0-beta.2",
  "author": "Lindsay Wardell",
  "description": "Bridge to render Elm modules in a Vue application",
  "main": "dist/elm-vue-bridge.*.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "postinstall": "elm-tooling install",
    "prebuild": "elm-tooling install",
    "prepare": "npm run build"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.3",
    "elm-tooling": "^1.6.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-elm": "^2.5.0",
    "vue-tsc": "^0.3.0"
  },
  "keywords": [
    "elm",
    "vue",
    "component"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lindsaykwardell/elm-vue-bridge.git"
  },
  "files": [
    "lib/**/*"
  ]
}