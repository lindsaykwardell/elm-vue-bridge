{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "double"],
    "trailing-comma": false,
    "arrow-parens": [false],
    "no-shadowed-variable": [false],
    "object-literal-key-quotes": [false],
    "curly": [false],
    "semicolon": [true]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/express-http-proxy/*/**", "src/express-http-proxy/*"]
  }
}
